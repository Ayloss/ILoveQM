<page class="page">
  <view class="head20">
    <text class="FONT1"> {{courseName}}</text>
    <image src='/resources/img/chooseschool.png' class="ninePointToCourseInfo" bindtap="detail" />
  </view>


  <view wx:for="{{seminarList}}">
    <view class='box3' id="{{item.id}}" bindtap="enterSeminar">
      <text class="FONT12"> {{item.name}}</text>
      <!--蓝色选择框-->
      <text class="FONT2" style="margin-left: 60%;margin-top: 3%"> {{item.startTime}}至{{item.endTime}}</text>
      <text class="FONT2" style="margin-left: 60%;margin-top: 12%">分组方式:{{item.groupingMethod}}</text>
      <text class="FONT2" style="margin-left: 60%;margin-top: 21%"> 成绩：{{item.grade}}     </text>
    </view>
  </view>



</page>